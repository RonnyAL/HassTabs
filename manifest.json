{
  "name": "HassTabs",
  "description": "Override the new tab page with an Home Assistant stuff!",
  "version": "0.4",
  "permissions": [
        "http://*",
		"https://*",
        "storage"
    ],
  "content_security_policy": "script-src 'self' https://code.jquery.com/ https://ajax.googleapis.com/; object-src 'self'",
  "incognito": "split",
    "persistent": false,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "icons": { "16": "favicon/icon16.png",
    "32": "favicon/icon32.png",
    "48": "favicon/icon48.png",
    "128": "favicon/icon128.png" },
  "manifest_version": 2
}
